{
  "name": "trashman",
  "main": "./lib/trashman",
  "version": "0.2.0",
  "description": "Clean up outdated package data on disk and Atom storage",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode none --watch",
    "lint": "gulp lint",
    "test": "npm run lint"
  },
  "keywords": [
    "apm",
    "local storage",
    "clean"
  ],
  "activationCommands": {
    "atom-workspace": [
      "trashman:delete-outdated-package-data"
    ]
  },
  "repository": "https://github.com/idleberg/atom-trashman",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-select-list": "^0.7.1",
    "fs-extra": "^8.0.1",
    "semver": "^6.3.0",
    "semver-compare": "^1.0.0"
  },
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeelint-stylish": "^0.1.2",
    "coffeescript": "^2.4.1",
    "gulp": "^4.0.2",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.3.0",
    "husky": "^3.0.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
}
